{
  "name": "next-payload-demo",
  "description": "Demo of Payload deployed as Vercel functions within NextJS",
  "version": "1.0.0",
  "license": "MIT",
  "type": "module",
  "yarn": {
    "supportedArchitectures": {
      "os": [
        "linux"
      ],
      "cpu": [
        "x64",
        "arm64"
      ]
    }
  },
  "pnpm": {
    "supportedArchitectures": {
      "os": [
        "linux"
      ],
      "cpu": [
        "x64",
        "arm64"
      ]
    }
  },
  "scripts": {
    "postinstall": "patch-package",
    "sst": "dotenvx run --env-file=.aws.env -- op run -- npx sst",
    "dev:next": "npm run sst bind next dev",
    "dev:nextExp": "next dev",
    "dev:sst": "npm run sst dev",
    "deploy": "npm run sst deploy -- --stage sandbox",
    "remove": "npm run sst remove",
    "console": "npm run sst console",
    "build:sst": "npm run sst build",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "generate:types": "cross-env PAYLOAD_CONFIG_PATH=src/payload/payload.config.ts payload generate:types"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.428.0",
    "@aws-sdk/lib-storage": "^3.428.0",
    "@dotenvx/dotenvx": "^0.15.4",
    "@faceless-ui/css-grid": "^1.2.0",
    "@faceless-ui/modal": "^2.0.1",
    "@payloadcms/db-mongodb": "^1.0.3",
    "@payloadcms/plugin-cloud-storage": "^1.1.1",
    "@payloadcms/plugin-seo": "^1.0.15",
    "@payloadcms/richtext-lexical": "^0.1.5",
    "@payloadcms/richtext-slate": "^1.2.1",
    "aws-crt": "^1.18.0",
    "critters": "^0.0.20",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "next": "^13.5.7-canary.37",
    "node-fetch": "^2.7.0",
    "null-loader": "^4.0.1",
    "payload": "^2.3.1",
    "payload-admin-bar": "^1.0.5",
    "sass": "^1.58.2",
    "sass-loader": "^13.2.0",
    "sharp": "^0.33.2",
    "slate": "^0.102.0"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "13.5.7-canary.37",
    "@payloadcms/bundler-webpack": "^1.0.2",
    "@payloadcms/next-payload": "^0.1.11",
    "@swc/core": "^1.4.1",
    "@swc/wasm": "^1.4.1",
    "@types/express": "^4.17.1",
    "@types/node": "^18.11.18",
    "@types/react": "^18.0.28",
    "aws-cdk-lib": "2.124.0",
    "constructs": "10.3.0",
    "copyfiles": "^2.4.1",
    "eslint": "8.31.0",
    "eslint-config-next": "13.3",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "nodemon": "^2.0.6",
    "patch-package": "^8.0.0",
    "prettier": "3.2.5",
    "sst": "^2.40.3",
    "ts-node": "^9.1.1",
    "typescript": "^4.8.4"
  }
}
